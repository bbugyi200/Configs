# ---------- Miscellaneous Bindings
map <C-[> feedkeys ":nohlsearch<Return>"
map <Esc> feedkeys ":nohlsearch<Return>"
map <Return> goto
map ,l feedkeys ":blist <Tab><C-u>blist"  # List Marks
map ,b focus_inputbar ":bmark "
map ,d feedkeys ":bdelete <Tab>"
map D feedkeys ":exec xzath adL<Return>"
map e feedkeys ":exec xzath ZdJH<Return>"
map E feedkeys ":exec xzath ZdJL<Return>"
map s feedkeys ":exec zsearch<Return>"
map S feedkeys ":exec zsearch --keys<Return>"
map R feedkeys ":exec zopen -R<Return>"
map o feedkeys ":exec zopen -x<Return>"
map O feedkeys ":exec zopen<Return>"
map w feedkeys ":exec xzath ZdH<Return>"
map W feedkeys ":exec xzath ZdL<Return>"
map x quit
map Z feedkeys "200zz"

# ---------- Disabled Bindings
unmap q

