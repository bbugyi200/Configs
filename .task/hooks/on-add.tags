#!/usr/bin/env python

"""Special tags on-add hook.

Process special tags when new tasks are added.
"""

import sys
import json

import tags

new_task = json.loads(sys.stdin.readline())

if __name__ == "__main__":
    new_task = tags.process_add_tags(new_task)

    print(json.dumps(new_task))
    sys.exit(0)
