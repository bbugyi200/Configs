{"v": "vim defaults.py ./*.py ./*.tags on-add.* on-modify.* $@", "debug": "sed -i.bak 's/_STREAM_LOGGING_LEVEL = logging.*/_STREAM_LOGGING_LEVEL = logging.DEBUG/g' log.py \"$@\"\nif [[ -f disabled.debug ]]; then\n    mv disabled.debug on-modify.debug \"$@\"\nfi", "nodebug": "sed -i.bak 's/_STREAM_LOGGING_LEVEL = logging.*/_STREAM_LOGGING_LEVEL = logging.INFO/g' log.py \"$@\"\nif [[ -f on-modify.debug ]]; then\n    mv on-modify.debug disabled.debug \"$@\"\nfi", "vdebug": "sed -i.bak 's/_STREAM_LOGGING_LEVEL = logging.*/_STREAM_LOGGING_LEVEL = logging.VDEBUG/g' log.py \"$@\"\nif [[ -f disabled.debug ]]; then\n    mv disabled.debug on-modify.debug \"$@\"\nfi"}